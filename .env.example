PONDER_RPC_URL_10="https://opt-mainnet.g.alchemy.com/v2/your-api-key"     # Optimism
PONDER_RPC_URL_100="https://rpc.gnosischain.com"                          # Gnosis Chain
PONDER_RPC_URL_137="https://polygon-mainnet.g.alchemy.com/v2/your-api-key" # Polygon
PONDER_RPC_URL_8453="https://base-mainnet.g.alchemy.com/v2/your-api-key"  # Base
PONDER_RPC_URL_42161="https://arb-mainnet.g.alchemy.com/v2/your-api-key"  # Arbitrum

# Optional: Main database URL (defaults to SQLite if not provided)
DATABASE_URL="postgresql://postgres@localhost:5432/agentscan"

# Optional: ABI database URL
# If not provided, ABI storage and embeddings will be disabled
ABI_DATABASE_URL="postgresql://postgres@localhost:5432/agentscan_abi"

# Optional: Redis URL
# If not provided, Redis caching will be disabled
REDIS_URL="redis://localhost:6379"

# Optional: OpenAI API key
# Required only if ABI_DATABASE_URL is set (for embeddings generation)
OPENAI_API_KEY="your-openai-api-key"

IS_LOCAL=true




